/*
 * Group 4
 * Food Class
 */
package fitnesstracker;

/**
 *Revision History
 * Revision 1: Kimberly Waddle, Initial Class, 3 December 2012
 */
public class Food {
    
    private String type;
    private String name;
    
    public Food(String foodName, String foodType){
        name = foodName;
        type = foodType;
    }
    
    public String getName(){
        return name;
    }
    public String getType(){
        return type;
    }    
}
